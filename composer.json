{
    "name": "demscript/monicredit",
    "description": "laravel sdk for monicredit",
    "license": "MIT",
    "keywords": [
        "php",
        "github",
        "laravel",
        "Open Source",
        "crypto",
        "defi",
        "laravel 6",
        "laravel 7",
        "laravel 8"
    ],
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Oluwademilade Ogegbo",
            "email": "demscript@gmail.com"
        }
    ],
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "../../../app/",
            "Demscript\\Monicredit\\": "src/",
            "Demscript\\Monicredit\\Tests\\": "tests"
        }
    },
    "require": {
        "php": "^7.3|^8.0|^8.1",
        "laravel/sanctum": "3.*",
        "laravel/sail": "1.*",
        "laravel/tinker": "2.*",
        "guzzlehttp/guzzle": "7.*",
        "fruitcake/laravel-cors": "3.*",
        "orchestra/testbench": "7.*",
        "phpunit/phpunit": "^9.*"
    },
    "require-dev": {
        "pestphp/pest": "^1.*",
        "pestphp/pest-plugin-laravel": "^1.*",
        "spatie/laravel-ignition": "^1.*"
    },
    "scripts": {
        "test": "./vendor/bin/pest"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Demscript\\Monicredit\\MonicreditServiceProvider"
            ],
            "aliases": {
                "Monicredit": "Demscript\\Monicredit\\Facades\\MonicreditFacade"
            }
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}